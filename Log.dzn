/*
 * Raft Command Log
 * 
 * Log of commands to agree upon with other servers
 * 
 * author: Eric Dortmans (e.dortmans@fontys.nl)
 */
 
interface ILog
{
	in void append();
	in void get();
	behaviour
	{
		on append: { }
		on get: { }
	}
}

component Log
{
	
	provides ILog ctrl;

	behaviour
	{
		on ctrl.append(): {}
		on ctrl.get(): {}
	}
}

