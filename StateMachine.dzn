import Types.dzn;
 
interface IStateMachine
{
	in void init();
	in void apply(string command);
	behaviour
	{
		on init: {}
		on apply: {}
	}
}

component StateMachine
{
	
	provides IStateMachine ctrl;

	behaviour
	{
		on ctrl.init(): {}
		on ctrl.apply(command): {}
	}
}